{
    "manifest_version": 2,
  
    "name": "Study Buddy",
    "description": "This extension will help users study, as well as help keep their mental health positive",
    "version": "1.0",

  
    "browser_action": {
     "default_icon": "./img/chicken-logo.png",
     "default_popup": "popup.html"
    },

    "background": {
     "scripts": ["./js/background_script.js"]
    },

    "permissions": [
     "activeTab"
     ]
  }